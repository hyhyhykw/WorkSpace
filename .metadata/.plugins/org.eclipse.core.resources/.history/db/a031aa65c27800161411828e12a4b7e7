package com.training;

public class While {
	public static void main(String[] args) {
		int a = 0;
		int sum1 = 0;
		int sum2 = 0;
		while (a <= 100) {
			if (a % 2 == 0) {
				sum1 += a;
			}
			if (a % 2 != 0) {
				sum2 += a;
			}
			a++
		}
		System.out.println(sum1);
		System.out.println(sum2);
	}
}

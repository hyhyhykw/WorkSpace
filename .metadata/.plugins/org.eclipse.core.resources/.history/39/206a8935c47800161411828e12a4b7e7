package com.training;

public class While {
	public static void main(String[] args) {
		int a = 0;
		int sum1 = 0;
		int sum2 = 0;
		while (a <= 100) {
			if (a % 2 == 0) {
				sum1 += a;
			}
			if (a % 2 != 0) {
				sum2 += a;
			}
			a++;
		}
		System.out.println("100以内偶数的和为" + sum1);
		System.out.println("100以内奇数的和为" + sum2);//结束
		
		int b=1;
		int n=1;
		while (b<=10) {
			n*=b;
			System.out.println(b+"!="+n);
			b++;
		}
		
		
		
	}
}
